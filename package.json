  "repository": "github:console-table-printer/console-table-printer",
  "description": "Printing pretty tables on console log",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "setup": "yarn",
    "build": "tsc",
    "format": "prettier --write \"**/*.{json,ts,tsx,yml,js,jsx}\"",
